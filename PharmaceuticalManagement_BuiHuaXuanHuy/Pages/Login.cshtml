@page
@model FUNewsManagement_BuiHuaXuanHuy.Pages.LoginModel
@{
	ViewData["Title"] = "Login";
}

<div class="text-center">
	<h1 class="display-4">Login Page</h1>
	<form method="post">
		<p>
			Email:
			<input type="text" name="txtEmail" />
		</p>
		<p>
			Password:
			<input type="password" name="txtPassword" />
		</p>
		<button type="submit" class="btn-primary" name="btnLogin" value="Login">Login</button>
	</form>
</div>